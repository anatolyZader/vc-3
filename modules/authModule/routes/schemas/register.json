{
  "type": "object",
  "$id": "schema:auth:register",
  "required": [
    "username",
    "email",
    "password"
  ],
  "properties": {
    "username": {
      "type": "string",
      "maxLength": 50
    },
    "email": {
      "type": "string",
      "maxLength": 20
    },
    "password": {
      "type": "string",
      "maxLength": 20
    }
  }
}