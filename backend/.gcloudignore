# Ignore development and testing files
.git/
.gitignore
README.md
*.md
.env
.env.*

# Ignore node_modules (CRITICAL - prevents uploading 90k+ dependency files)
node_modules/

# Ignore test files
_tests_/
test/
tests/
*.test.js
*.spec.js

# Ignore development tools
.eslintrc*
eslint.config.js
jest.config.js

# Ignore logs and temporary files
logs/
*.log
cloud_run_logs.txt
logs_run.txt

# Ignore large files that aren't needed for runtime
bfg.jar
cloud-sql-proxy
*.zip
*.tar.gz

# Additional exclusions for smaller builds
*.log.*
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.DS_Store
Thumbs.db
